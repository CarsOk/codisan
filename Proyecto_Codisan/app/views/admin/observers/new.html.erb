<div class="container_observadores">
  <%= button_to "Volver", admin_observers_url(@observer), method: :get, class: "btn btn-primary", id: "btn-back" %>
   <div class="row_buscardor">
      <h5 id = "h5">Estudiantes</h5>
      <%= form_tag admin_observers_path, method: :get do %>
        <input name = "document", placeholder = "Documento">
        <%= submit_tag "buscar" %>
      <%end%> 
    </div>
    <div class="col">
        <%= render "form" %>    
    </div>
  </div>
</div>